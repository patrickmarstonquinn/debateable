<div class="page-header">
  <h3>Debate #<%= @debate.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Liburl</dt>
      <dd><%= @debate.liburl %></dd>

      <dt>Libtitle</dt>
      <dd><%= @debate.libtitle %></dd>

      <dt>Conurl</dt>
      <dd><%= @debate.conurl %></dd>

      <dt>Contitle</dt>
      <dd><%= @debate.contitle %></dd>

      <dt>User</dt>
      <dd><%= @debate.user_id %></dd>

      <dt>Topic</dt>
      <dd><%= @debate.topic %></dd>

      <dt>Electionid</dt>
      <dd><%= @debate.electionid %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/debates" class="btn btn-primary">
        Back
      </a>
      <a href="/debates/<%= @debate.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_debate/<%= @debate.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
